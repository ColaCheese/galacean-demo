{
	"name": "galacean-demo",
	"private": true,
	"version": "0.0.1",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc --noEmit --skipLibCheck && vite build",
		"serve": "vite preview",
		"commit": "git-cz",
		"major": "standard-version -r major --preset gitmoji-config",
		"minor": "standard-version -r minor --preset gitmoji-config",
		"patch": "standard-version -r patch --preset gitmoji-config"
	},
	"dependencies": {
		"@galacean/engine": "^1.0.4",
		"@galacean/engine-spine": "1.0.0-beta.0",
		"@galacean/engine-toolkit-controls": "^1.0.0",
		"@types/dat.gui": "^0.7.10",
		"dat.gui": "^0.7.9",
		"vue": "^3.3.4"
	},
	"devDependencies": {
		"@types/node": "^20.6.0",
		"@vitejs/plugin-vue": "^2.3.4",
		"commitizen": "^4.3.0",
		"commitlint": "^17.7.1",
		"commitlint-config-gitmoji": "^2.3.1",
		"conventional-changelog-gitmoji-config": "^1.5.2",
		"cz-custom": "^0.0.2",
		"husky": "^8.0.3",
		"standard-version": "^9.5.0",
		"typescript": "^4.9.5",
		"vite": "^2.9.16",
		"vue-tsc": "^0.29.8"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-custom"
		}
	}
}
